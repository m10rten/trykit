{
  "name": "trykit",
  "version": "0.3.0",
  "description": "Simple magic-like utilities to ease your development experience",
  "private": false,
  "types": "./dist/index.d.ts",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "homepage": "https://github.com/m10rten/trykit#readme",
  "repository": {
    "type": "git",
    "url": "https://github.com/m10rten/trykit.git"
  },
  "bugs": {
    "url": "https://github.com/m10rten/trykit/issues"
  },
  "readme": "",
  "scripts": {
    "build": "tsup --format esm,cjs --dts",
    "lint": "eslint src/**/*.ts",
    "test": "jest",
    "typecheck": "tsc --noEmit",
    "test:coverage": "jest --coverage",
    "test:ci": "jest --ci --passWithNoTests",
    "prettier:check": "prettier --check .",
    "format": "prettier --write .",
    "generate": "echo 'Not implemented yet.'",
    "precheck": "pnpm typecheck && pnpm lint",
    "verify": "pnpm build && pnpm test",
    "release": "changeset publish",
    "change": "changeset",
    "prepare": "husky"
  },
  "type": "commonjs",
  "keywords": [],
  "author": "m10rten",
  "license": "MIT",
  "devDependencies": {
    "@changesets/cli": "2.27.10",
    "@eslint/js": "9.16.0",
    "@ianvs/prettier-plugin-sort-imports": "4.4.0",
    "@types/eslint__js": "8.42.3",
    "@types/jest": "29.5.14",
    "@types/node": "22.10.1",
    "eslint": "9.15.0",
    "husky": "9.1.7",
    "jest": "29.7.0",
    "ts-jest": "29.2.5",
    "ts-node": "10.9.2",
    "tsup": "8.3.5",
    "tsx": "4.19.2",
    "typescript": "5.7.2",
    "typescript-eslint": "8.16.0",
    "zod": "3.23.8"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.js",
      "import": "./dist/index.mjs"
    },
    "./safe-try": {
      "types": "./dist/safe-try.d.ts",
      "require": "./dist/safe-try.js",
      "import": "./dist/safe-try.mjs"
    },
    "./retry": {
      "types": "./dist/retry.d.ts",
      "require": "./dist/retry.js",
      "import": "./dist/retry.mjs"
    },
    "./merge": {
      "types": "./dist/merge.d.ts",
      "require": "./dist/merge.js",
      "import": "./dist/merge.mjs"
    },
    "./try-parse": {
      "types": "./dist/try-parse.d.ts",
      "require": "./dist/try-parse.js",
      "import": "./dist/try-parse.mjs"
    },
    "./try-to": {
      "types": "./dist/try-to.d.ts",
      "require": "./dist/try-to.js",
      "import": "./dist/try-to.mjs"
    },
    "./package.json": "./package.json"
  },
  "typesVersions": {
    "*": {
      "safe-try": [
        "dist/safe-try.d.ts"
      ],
      "retry": [
        "dist/retry.d.ts"
      ],
      "try-parse": [
        "dist/try-parse.d.ts"
      ]
    }
  },
  "files": [
    "dist",
    "package.json",
    "LICENSE",
    "README.md"
  ],
  "publishConfig": {
    "access": "public"
  }
}
